social list list-of-socials:

-- ftd.column:
width: fill-container

-- ftd.image:
src: $post.meta.post-image

-- ftd.row:
wrap: true
spacing.fixed.percent: 5
width: fill-container
margin-top.px: 50

-- col:
width: 25

-- ftd.text: $post.meta.published-on
role: $inherited.types.label-small
color: $inherited.colors.text

-- user: $post.meta.author.title
src: $post.meta.author.image
body: $post.meta.author.profile
marginTop: 26

-- end: user

-- social-wrapper: Share
list-of-socials: $post.list-of-socials
marginTop: 38

-- end: social-wrapper

-- end: col

-- ftd.column:
margin-top.px: 50
width.fixed.percent: 70
children: $post.child

-- end: ftd.column

-- end: ftd.row

-- end: ftd.column

-- end: post









-- component unorderedList:
unordList list unord-list:

-- ftd.column:
width: fill-container

-- ftd.text: $obj.title
$loop$: $unorderedList.unord-list as $obj
role: $inherited.types.copy-regular
color: $inherited.colors.text

-- end: ftd.column

-- end: unorderedList









-- record unordList:
caption title:










-- component br:
integer width: $width

-- ftd.column:
width: fill-container
border-width.px: 1
border-color: $inherited.colors.border-strong
margin-vertical.px: 50
max-width.fixed.px: $br.width

-- end: ftd.column

-- end: br









